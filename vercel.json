{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/about",
        "dest": "/about.html"
      },
      {
        "src": "/portfolio",
        "dest": "/portfolio.html"
      },
      {
        "src": "/services",
        "dest": "/services.html"
      },
      {
        "src": "/work",
        "dest": "/work.html"
      },
      {
        "src": "/contact",
        "dest": "/contact.html"
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }
  